<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>
<?import com.jfoenix.controls.JFXScrollPane?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.shape.Path?>
<fx:root xmlns:fx="http://javafx.com/fxml/1"
         xmlns="http://javafx.com/javafx/8.0.76-ea"
         type="StackPane">
    <JFXScrollPane id="textFieldScrollPane" visible="false">
        <StackPane visible="false">
            <Path id="shape" StackPane.alignment="CENTER_LEFT" strokeType="INSIDE" visible="false"/>

            <Label id="label" styleClass="sub-caption" StackPane.alignment="CENTER_LEFT" visible="false"/>

            <JFXTextField id="textField" styleClass="sub-caption" StackPane.alignment="CENTER_LEFT" mouseTransparent="true"/>
        </StackPane>
        <StackPane>
            <Path id="shape1" StackPane.alignment="CENTER_LEFT" strokeType="INSIDE" visible="false"/>

            <Label id="label1" styleClass="sub-caption" StackPane.alignment="CENTER_LEFT" visible="false"/>

            <JFXTextField id="textField1" styleClass="sub-caption" StackPane.alignment="CENTER_LEFT" mouseTransparent="true"/>
        </StackPane>
        <StackPane>
            <Path id="shape2" StackPane.alignment="CENTER_LEFT" strokeType="INSIDE" visible="false"/>

            <Label id="label2" styleClass="sub-caption" StackPane.alignment="CENTER_LEFT" visible="false"/>

            <JFXTextField id="textField2" styleClass="sub-caption" StackPane.alignment="CENTER_LEFT" mouseTransparent="true"/>
        </StackPane>
    </JFXScrollPane>
</fx:root>